{
  "app_name": "Wadeulwadeul Heroes API",
  "environment": "production",
  "debug": false,
  "cors": {
    "allow_origins": ["*"],
    "allow_credentials": true,
    "allow_methods": ["*"],
    "allow_headers": ["*"]
  },
  "database": {
    "host": "your-database-host",
